# Production Beta Configuration
# v2.1.1-beta with PLL streaming enabled

MODE=AGENT
BETA_FEATURES=true

# API Keys (set in deployment)
OPENROUTER_API_KEY=
SERVER_API_KEY=

# Server
SERVER_PORT=3000
BIND_ADDRESS=0.0.0.0
PUBLIC_URL=https://your-domain.com

# Security
REQUIRE_HTTPS=true
RATE_LIMIT_MAX_REQUESTS=100
OAUTH_ENABLED=true
AUTH_JWKS_URL=

# PLL Streaming (production defaults)
PLL_ENABLE=true
PLL_TARGET_TOKEN_RATE=32
PLL_MAX_CONCURRENCY=6
PLL_GAIN=0.5

# Compression
COMPRESSION_ENABLE=true
COMPRESSION_TARGET_TOKENS=3200

# Storage
PGLITE_DATA_DIR=/data/researchAgentDB
REPORT_OUTPUT_PATH=/data/research_outputs

# Performance
PARALLELISM=4
ENSEMBLE_SIZE=2

# Logging
LOG_LEVEL=info

