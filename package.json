{
  "name": "@terminals-tech/openrouter-agents",
  "version": "2.1.1-beta",
  "description": "OpenRouter MCP server for research agents",
  "main": "src/server/mcpServer.js",
  "bin": {
    "openrouter-agents": "bin/openrouter-agents.js",
    "openrouter-agents-mcp": "bin/openrouter-agents-mcp.js"
  },
  "files": [
    "src/**",
    "bin/**",
    "config.js",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "start": "node src/server/mcpServer.js",
    "start:mcp": "node bin/openrouter-agents-mcp.js",
    "dev": "nodemon src/server/mcpServer.js",
    "stdio": "node src/server/mcpServer.js --stdio",
    "test": "node tests/test-all-tools.js",
    "test:stream": "node --experimental-modules tests/streaming-contract.spec.js",
    "test:perf": "node tests/perf-bench.js",
    "test:mcp": "node tests/run-mcp-tests.js",
    "test:e2e": "node tests/e2e-user-journeys.spec.js",
    "test:beta": "node tests/beta-features-isolation.spec.js",
    "test:sdk": "node tests/mcp-sdk-compatibility.spec.js",
    "test:suite": "node scripts/production-readiness-check.js",
    "test:all-beta": "npm run test:e2e && npm run test:beta && npm run test:sdk",
    "qa:report": "node scripts/generate-qa-report.js",
    "gen:examples": "node scripts/generate-examples.js",
    "gen:docs": "node scripts/gen-docs.js",
    "gen:diagram": "mmdc -i docs/diagram-architecture.mmd -o docs/diagram-architecture.svg"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Tej Desai <admin@terminals.tech> (https://terminals.tech)",
  "homepage": "https://terminals.tech",
  "license": "MIT",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "model context protocol",
    "openrouter",
    "agents",
    "research",
    "pglite",
    "pgvector",
    "oauth2",
    "websocket",
    "sse",
    "streaming",
    "llm",
    "ai"
  ],
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@electric-sql/pglite": "^0.2.17",
    "@huggingface/hub": "^2.6.11",
    "@modelcontextprotocol/sdk": "1.17.4",
    "@terminals-tech/core": "^0.1.1",
    "@terminals-tech/embeddings": "^0.1.0",
    "@terminals-tech/graph": "^0.1.0",
    "@xenova/transformers": "^2.17.2",
    "axios": "^1.12.0",
    "csv-parse": "^5.6.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "eventsource-parser": "1.1.2",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "fast-xml-parser": "^5.0.9",
    "jose": "^5.9.6",
    "jsdom": "^23.0.1",
    "node-cache": "^5.1.2",
    "node-fetch": "^2.7.0",
    "node-llama-cpp": "^3.14.0",
    "playwright": "^1.56.0",
    "tar": "^6.2.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "assert": "^2.1.0",
    "nodemon": "^3.0.1"
  },
  "directories": {
    "doc": "docs",
    "test": "tests"
  }
}
