    "mcp",
    "model context protocol",
    "openrouter",
    "agents",
    "research",
    "pglite",
    "pgvector"
  ],
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@electric-sql/pglite": "^0.2.17",
    "@modelcontextprotocol/sdk": "1.17.4",
    "@xenova/transformers": "^2.17.2",
    "axios": "^1.6.2",
    "csv-parse": "^5.6.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "eventsource-parser": "1.1.2",
    "express": "^4.18.2",
    "fast-xml-parser": "^5.0.9",
    "jsdom": "^23.0.1",
    "node-cache": "^5.1.2",
    "node-fetch": "^2.7.0",
    "tar": "^6.2.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
