{
  "name": "@terminals-tech/openrouter-agents",
  "version": "1.5.1",
  "description": "OpenRouter MCP server for research agents",
  "main": "src/server/mcpServer.js",
  "bin": {
    "openrouter-agents": "src/server/mcpServer.js"
  },
  "scripts": {
    "start": "node src/server/mcpServer.js",
    "dev": "nodemon src/server/mcpServer.js",
    "stdio": "node src/server/mcpServer.js --stdio",
    "gen:examples": "node scripts/generate-examples.js",
    "gen:docs": "node scripts/gen-docs.js",
    "gen:diagram": "mmdc -i docs/diagram-architecture.mmd -o docs/diagram-architecture.svg"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Tej Desai <admin@terminals.tech> (https://terminals.tech)",
  "homepage": "https://terminals.tech",
